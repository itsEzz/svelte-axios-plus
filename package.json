{
	"name": "svelte-axios-plus",
	"version": "1.0.1",
	"description": "Axios with some additional features to make working with request even more simple and better.",
	"homepage": "https://github.com/itsEzz/svelte-axios-plus",
	"repository": {
		"type": "git",
		"url": "https://github.com/itsEzz/svelte-axios-plus"
	},
	"bugs": {
		"url": "https://github.com/itsEzz/svelte-axios-plus/issues"
	},
	"keywords": [
		"axios",
		"svelte"
	],
	"license": "ISC",
	"author": {
		"name": "Adrian Gast"
	},
	"scripts": {
		"dev": "vite dev",
		"dev-debug": "vavite-loader vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"axios": ">=1.0.0",
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.30.3",
		"@sveltejs/package": "^2.2.5",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.16.0",
		"@vavite/node-loader": "^4.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.2.7",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "^4.5.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"lru-cache": "^10.1.0"
	}
}
